{
  "name": "Dapphub",
  "icon": "src/Dapphub.ico",
  "version": "0.5.0",
  "license": "UNLICENSED",
  "main": "main/index.js",
  "author": {
    "name": "Dapphub LLC."
  },
  "build": {
    "mac": {
      "category": "your.app.category.type",
      "icon": "src/Dapphub.icns"
    },
    "dmg": {
      "title": "Dapphub"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Travis-Sherman",
        "repo": "https://github.com/Travis-Sherman/vite-reactts-electron-starter"
      }
    ],
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  },
  "description": "Dapphub is an application built towards a more empowered and equitable digital future with OnChain technology.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/Travis-Sherman/vite-reactts-electron-starter"
  },
  "productName": "Dapphub",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@electron-forge/cli": "^7.3.1",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^1.6.8",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.8",
    "ethers": "^6.11.1",
    "firebase": "^10.9.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-slider": "^2.0.6",
    "swiper": "^11.0.7",
    "vite": "^2.8.6",
    "web3": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^24.13.3",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.6.2"
  }
}
